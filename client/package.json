{
  "name": "homma-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview --port 8000",
    "lint": "eslint src --ext ts,tsx,json --report-unused-disable-directives",
    "lint:fix": "eslint --fix --ext ts,tsx,json",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "precommit": "npm run format && npm run lint"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "@material-tailwind/react": "^2.1.9",
    "@react-oauth/google": "^0.12.1",
    "@reduxjs/toolkit": "^2.2.4",
    "jwt-decode": "^4.0.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.16.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-ts-tradingview-widgets": "^1.2.5"
  },
  "devDependencies": {
    "@types/react": "18.2.42",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
